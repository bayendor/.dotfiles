[user]
	name = David Bayendor
	email = dbayendor@gmail.com
	initials = DB
[credential]
	helper = osxkeychain
[push]
	default = current
[alias]
	co = checkout
  st = status
  sub = submodule foreach git pull origin master
  ci = commit -m
  shas = log --pretty=format:\"%H\"
  hist = log --pretty=format:\"%Cgreen%h%Creset %Cblue%ad%Creset %s%C(yellow)%d%Creset %Cblue[%an]%Creset\" --graph --date=short
  authors = ! git log --format=\"%ae %an\" | sort -u | column -t
[core]
	editor = /usr/local/bin/mate -w
	excludesfile = /Users/David/.gitignore_global
[color]
	ui = true
	diff = auto
	status = auto
	branch = auto
[diff]
	tool = opendiff
[merge]
	tool = opendiff
[git-tmbundle]
  gitx-path = /Applications/GitX.app
[push]
  default = upstream
